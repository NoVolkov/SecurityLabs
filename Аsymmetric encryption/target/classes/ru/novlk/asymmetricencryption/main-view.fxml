<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" nodeOrientation="LEFT_TO_RIGHT" prefHeight="200.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.novlk.asymmetricencryption.MainController">
   <children>
      <Label fx:id="txt_keysExist" text="Ключи отсутствуют" />
      <Button mnemonicParsing="false" onAction="#onClick_generateKeys" text="Сгенерировать ключи" />
      <Button fx:id="btn_export" disable="true" mnemonicParsing="false" onAction="#onClick_export" text="Экспортировать (куда-то)" />
      <Button fx:id="btn_import" mnemonicParsing="false" onAction="#onClick_import" text="Импортировать (откуда-то)" />
      <Button fx:id="btn_encryption" disable="true" mnemonicParsing="false" onAction="#onClick_encryption" text="Зашифровать" />
      <Button fx:id="btn_decryption" disable="true" mnemonicParsing="false" onAction="#onClick_decryption" text="Расшифровать" />
   </children>
</VBox>
